  //set the Requested for value to the currently logged in user.
	if(g_form.isNewRecord()){
		var creePar = g_form.getValue('opened_by');
		var ga = new GlideAjax('GetUserInfo');
		ga.addParam('sysparm_name', 'getUserGroupId');
		ga.addParam('sysparm_creePar', creePar);
		ga.getXMLAnswer(function(answer){
			jslog('Group Id: ' + answer);
			g_form.setValue('u_propritaire_du_ticket', answer);
		});
    }
}]]></script>
